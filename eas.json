{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "store",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "releaseChannel": "production",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "distribution": "store",
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.3-xcode-16.2",
        "env": {
          "EXPO_USE_MODULAR_HEADERS": "true"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
